public class Fourth {

	public static void Main(String[] args)
	{
		int arr[] = {1,0,1,1,0,0}, b;
		
		for(int i = 0;i< arr.length -1; i++)
		{
			for(int j = 0; j< arr.length -1; j++)
			{
				if(arr[j] > arr[j+1])
				{
					b = arr[j];
					arr[j] = arr[j+1];
					arr[j+1] = b;
				}
			}
		}
		
		for(int i =0;i< arr.length; i++)
			System.out.print(arr[i]);
	}
}
